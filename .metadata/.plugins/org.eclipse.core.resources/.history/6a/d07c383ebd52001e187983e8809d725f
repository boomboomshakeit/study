package tut05;

public class LoofEx5 {

	public static void main(String[] args) {
		
		//q를 누르기 전에는 프로그램은 무한루프
		//q 입력하면 프로그램 종료
		//ui : a 누르면 더하기, q 종료
		//a를 누르면 두 정수를 입력 받아 더하기 한 후 출력
		
		boolean run = true;
		
		while(run) {
			System.out.println("=========================");
			System.out.println("1. 더하기(a) | 2. 종료q)");
			System.out.println("=========================");
		}
		
		
	}

}
