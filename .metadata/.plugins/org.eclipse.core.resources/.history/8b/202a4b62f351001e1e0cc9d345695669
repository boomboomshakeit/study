package tut04;

public class ContEx2 {

	public static void main(String[] args) {
		//컨트롤 쉬프트 에
		
		/*
		 학점 91 ~ 100 A등급
		 81 ~ 90 B등급
		 71 ~ 80 C등급
		 나머지는 F등급으로 표시하시오
		 */
		
		int jumsu = 88;
		
		if (jumsu <= 100 && jumsu > 91) {
			System.out.println("A등");
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
