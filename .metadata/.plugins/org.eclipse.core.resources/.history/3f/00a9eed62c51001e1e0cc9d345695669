package tut03;

public class LogicEx {

	public static void main(String[] args) {
		
		boolean val1 = true;
		boolean val2 = false;
		
		//&& : 조건 모두 = AND = 이면서, 이고
		// 조건1을 만족하면 조건2도 만족하는 결과를 출력하시오.
		System.out.println(val1 && val2);
		
		
		//|| : 조건 중 하나라도 = OR = 또는
		// 조건1도 만족하거나 조건2도 만족하는 결과를 출력하시오.
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
