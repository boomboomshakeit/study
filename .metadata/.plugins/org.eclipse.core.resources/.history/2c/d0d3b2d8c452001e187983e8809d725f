package tut05;

import java.util.Scanner;

public class LoopEx6 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		boolean star = true;
		
		while(star) {
			System.out.println("t================");
			System.out.println("스타벅스 커피 자동 판매");
			System.out.println("t================");
		}
		
		
		
		
		
		
		
		
		
		
	}

}
