package tut02;

public class PrintEx {
	
	public static void main(String[] args) {
		
		//정수 5와 8의 합을 출력하시오.
		//변수 선언 초기화 = 선언과 초기화를동
		
//		int num1, num2;
//		num1 = 5;
//		num2 = 8;
		
		//변수이름 첫글자는 소문자
		int num1 = 5;
		int num2 = 8;
		int sum;
		//System.in - scanner 사용 시
		//스테틱메서드
		
		//결과값을 변수에 저장 후 출력
		
		sum = num1 + num2;
		
		System.out.println(num1 + num2);
	}

}